package com.nt.service;

import java.util.List;

import com.nt.entity.Student;

public interface IStudentMgmtService {
	List<Student> getAllStudents();
	Student addStudent(Student d);
	
	Student getRecordByName(String name);
	Student getRecordByEmail(String email);
	Student getRecordByMobile(Integer mob);
	Student getRecordByCountry(String country);
	Student getRecordByState(String state);
	Student getRecordByDistrict(String district);
}
