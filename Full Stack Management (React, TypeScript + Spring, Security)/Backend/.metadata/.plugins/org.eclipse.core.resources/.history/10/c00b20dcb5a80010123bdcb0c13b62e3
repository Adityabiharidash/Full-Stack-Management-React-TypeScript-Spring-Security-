package com.nt.service;

import java.util.List;

import com.nt.entity.Student;

public interface IStudentMgmtService {
	List<Student> getAllStudents();
	Student addStudent(Student d);
	
	List<Student> getRecordByName(String name);
	List<Student> getRecordByEmail(String email);
	List<Student> getRecordByMobile(Integer mob);
	List<Student> getRecordByCountry(String country);
	List<Student> getRecordByState(String state);
	List<Student> getRecordByDistrict(String district);
}
